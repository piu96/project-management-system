{
  "name": "project-management-api",
  "version": "1.0.0",
  "description": "A comprehensive project management application built with Node.js, Express.js, and MongoDB",
  "main": "./bin/www",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bin/www",
    "dev": "cross-env NODE_ENV=development nodemon ./bin/www",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "seed": "node src/utils/seeders/index.js",
    "clean": "node src/utils/cleanDatabase.js"
  },
  "keywords": [
    "project-management",
    "task-management",
    "workspace",
    "time-tracking",
    "collaboration",
    "nodejs",
    "express",
    "mongodb",
    "mongoose"
  ],
  "author": "SUTRISHNA BERA",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.0-rc.4",
    "nodemailer": "^6.9.16",
    "slugify": "^1.6.6",
    "winston": "^3.16.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "@jest/globals": "^29.7.0"
  },
  "overrides": {
    "glob": "^10.3.10",
    "inflight": "npm:@npmcli/promise-spawn@^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/project-management-api.git"
  }
}
